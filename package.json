{
  "name": "kzv-icons",
  "version": "1.0.5",
  "description": "KZV icons and utilities for syncing them with Figma",
  "main": "dist/index",
  "scripts": {
    "test": "",
    "optimize": "node ./scripts/optimize.js",
    "download": "node ./scripts/download.js",
    "rename": "node ./scripts/rename.js",
    "componentize": "node ./scripts/componentize.js",
    "sync": "npm run download && npm run rename && npm run optimize && npm run componentize",
    "build": "babel ./src/components -d ./dist && node ./scripts/copyFiles.js",
    "release": "git checkout release && npm run build && npm version patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeonSkrilec/kzv-icons.git"
  },
  "author": "Leon Å krilec",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LeonSkrilec/kzv-icons/issues"
  },
  "homepage": "https://github.com/LeonSkrilec/kzv-icons#readme",
  "dependencies": {
    "fs-extra": "^9.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@svgr/core": "^5.5.0",
    "babel-preset-env": "^1.7.0",
    "change-case": "^4.1.2",
    "copy-dir": "^1.3.0",
    "dotenv": "^8.2.0",
    "figma-api-exporter": "0.0.2",
    "inquirer": "^8.0.0",
    "svgo": "^2.2.2"
  }
}
